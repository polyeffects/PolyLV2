@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix ui:    <http://lv2plug.in/ns/extensions/ui#> .
@prefix epp:   <http://lv2plug.in/ns/ext/port-props#> .
@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
<http://polyeffects.com/lv2/harm_trem_ext>
       a lv2:Plugin ;
       doap:name "harm_trem_ext" ;
       lv2:binary <harm_trem_ext.so> ;
       lv2:optionalFeature epp:supportsStrictBounds ;
       lv2:optionalFeature lv2:hardRtCapable ;
    lv2:port [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 0 ;
	lv2:symbol "CrossoverFreq_0" ;
	lv2:name "CrossoverFreq" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 9980 ;
	lv2:default 800 ;
	lv2:minimum 20 ;
	lv2:maximum 10000 ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 1 ;
	lv2:symbol "depth_1" ;
	lv2:name "depth" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 100 ;
	lv2:default 0 ;
	lv2:minimum 0 ;
	lv2:maximum 1 ;
    ] , [
	a lv2:InputPort ;
	a lv2:AudioPort ;
	lv2:index 2 ;
	lv2:symbol "in0" ;
	lv2:name "in" ;
    ] , [
	a lv2:InputPort ;
	a lv2:CVPort ;
	lv2:index 3 ;
	lv2:symbol "lfo_in1" ;
	lv2:name "LFO CV" ;
    ] , [
	a lv2:OutputPort ;
	a lv2:AudioPort ;
	lv2:index 4 ;
	lv2:symbol "out0" ;
	lv2:name "out" ;
    ]
.
