<http://polyeffects.com/lv2/basic_modular#difference> 
	a lv2:Plugin ,
		lv2:AmplifierPlugin ;
	doap:name "difference" ;
        doap:license <http://opensource.org/licenses/GPL-2.0> ;
        doap:description "difference of values";
        doap:maintainer [
                foaf:name "Poly Effects" ;
        ] ;
        lv2:minorVersion 0 ;
        lv2:microVersion 1 ;
        rdfs:comment "Differences" ;
	lv2:optionalFeature lv2:hardRTCapable ;

	lv2:port [
		a lv2:ControlPort,
			lv2:InputPort ;
		lv2:index 0 ;
		lv2:maximum 1 ;
		lv2:minimum 0 ;
		lv2:default 0 ;
		lv2:name "a" ;
		lv2:symbol "a" ;
	] , [
		a lv2:ControlPort ,
			lv2:InputPort ;
		lv2:index 1 ;
		lv2:maximum 1 ;
		lv2:minimum 0 ;
		lv2:default 0 ;
		lv2:name "b" ;
		lv2:symbol "b" ;
	] , [
		a lv2:CVPort ,
			lv2:OutputPort ;
		lv2:index 2 ;
		lv2:name "Output" ;
		lv2:symbol "out" ;
	] , [
		a lv2:CVPort,
			lv2:InputPort ;
		lv2:index 3 ;
		lv2:maximum 1 ;
		lv2:minimum 0 ;
		lv2:default 0 ;
		lv2:name "A CV" ;
		lv2:symbol "a_cv" ;
	] , [
		a lv2:CVPort ,
			lv2:InputPort ;
		lv2:index 4 ;
		lv2:maximum 1 ;
		lv2:minimum 0 ;
		lv2:default 0 ;
		lv2:name "B CV" ;
		lv2:symbol "b_cv" ;
	] .

